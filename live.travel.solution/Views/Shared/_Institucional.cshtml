@{
    Layout = "";
}

<div class="section text-center">
    <div class="row">
        <div class="col-md-8 ml-auto mr-auto">
            <h2 class="h2 marginB text-uppercase">marcas parceiras</h2>
            <!-- Carroseul -->
            <div class="row marginB">
                <div class="col-md-12 mr-auto ml-auto">
                    <!-- Carousel Card -->
                    <div class="card-raised card-carousel">
                        <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel" data-interval="2000">
                            <div class="carousel-inner">
                                <div class="carousel-item active">
                                    <img class="col imgsize" src="~/assets/img/partner/partner01.png" alt="First slide">
                                    <img class="col imgsize" src="~/assets/img/partner/partner02.png" alt="First slide">
                                    <img class="col imgsize" src="~/assets/img/partner/partner03.png" alt="First slide">
                                </div>
                                <div class="carousel-item">
                                    <img class="col imgsize" src="~/assets/img/partner/partner04.png" alt="First slide">
                                    <img class="col imgsize" src="~/assets/img/partner/partner05.png" alt="First slide">
                                    <img class="col imgsize" src="~/assets/img/partner/partner06.png" alt="First slide">
                                </div>
                                <div class="carousel-item">
                                    <img class="col imgsize" src="~/assets/img/partner/partner07.png" alt="First slide">
                                    <img class="col imgsize" src="~/assets/img/partner/partner08.png" alt="First slide">
                                    <img class="col imgsize" src="~/assets/img/partner/partner09.png" alt="First slide">
                                </div>
                                <div class="carousel-item">
                                    <img class="col imgsize" src="~/assets/img/partner/partner11.png" alt="First slide">
                                    <img class="col imgsize" src="~/assets/img/partner/partner12.png" alt="First slide">
                                    <img class="col imgsize" src="~/assets/img/partner/partner10.png" alt="First slide">
                                </div>
                                <div class="carousel-item">
                                    <img class="col imgsize" src="~/assets/img/partner/partner14.png" alt="First slide">
                                    <img class="col imgsize" src="~/assets/img/partner/partner15.png" alt="First slide">
                                    <img class="col imgsize" src="~/assets/img/partner/partner13.png" alt="First slide">
                                </div>
                                <div class="carousel-item">
                                    <img class="col imgsize" src="~/assets/img/partner/partner17.png" alt="First slide">
                                    <img class="col imgsize" src="~/assets/img/partner/partner16.png" alt="First slide">
                                    <img class="col imgsize" src="~/assets/img/partner/partner18.png" alt="First slide">
                                </div>
                                <div class="carousel-item">
                                    <img class="col imgsize" src="~/assets/img/partner/partner19.png" alt="First slide">
                                </div>
                            </div>
                            <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                                <i class="material-icons">keyboard_arrow_left</i>
                                <span class="sr-only">Previous</span>
                            </a>
                            <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                                <i class="material-icons">keyboard_arrow_right</i>
                                <span class="sr-only">Next</span>
                            </a>
                        </div>
                    </div>
                    <!-- End Carousel Card -->
                </div>
            </div>
        </div>
    </div>
    <hr class="marginB" />
    <div class="row marginB">
        <div class="col-md-8 ml-auto mr-auto">
            <h2 class="h2 text-uppercase marginB">sede historia</h2>
            <div class="video-container rounded drop-shadow"><iframe width="853" height="480" src="https://www.youtube.com/embed/z9Ul9ccDOqE" frameborder="0" allowfullscreen></iframe></div>
        </div>
    </div>
